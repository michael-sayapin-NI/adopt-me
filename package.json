{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "intro to react v7",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,html}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "build": "parcel build",
    "start": "npm -s run build && node dist/backend/index.js"
  },
  "author": "Michael Sayapin",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.2",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10"
  },
  "dependencies": {
    "express": "^4.17.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "browserslist": [
    "last 2 chrome versions"
  ],
  "targets": {
    "frontend": {
      "source": [
        "src/index.html"
      ],
      "publicUrl": "/frontend"
    },
    "backend": {
      "source": [
        "server/index.js"
      ],
      "optimize": false,
      "context": "node",
      "engines": {
        "node": ">=14"
      }
    }
  }
}
